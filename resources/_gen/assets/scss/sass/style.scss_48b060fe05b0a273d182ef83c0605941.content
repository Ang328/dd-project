body{background-color:#fff;font-family:national,sans-serif}h1{font-weight:500;font-size:3rem;line-height:3.125rem;font-feature-settings:"pnum","lnum"}h3{font-weight:300;font-size:1.75rem;line-height:2.125rem}h4{font-weight:500;font-size:1.5rem;line-height:1.75rem;text-transform:uppercase}h6{font-weight:600;font-size:.75rem;line-height:1rem;letter-spacing:.02142856rem;text-transform:uppercase}p{font-weight:300;font-size:1rem;line-height:1.1875rem}p.bold{font-weight:500;letter-spacing:.3px}p.light{font-weight:200}p.small{text-transform:capitalize;font-size:.875rem;line-height:1rem}.sidebar{background-color:#724eef;width:100vw}@media(min-width:768px){.sidebar{min-width:10.375rem;max-width:11.875rem}}.sidebar .nav-link{text-transform:uppercase;color:#fff;padding:.875rem 1.5rem}.sidebar .navbar-brand{margin:2.5rem 0 2.625rem 1.5rem}.main{margin:2.5rem 1.5rem}.main .pricing{margin-bottom:30px}.main .pricing h3{margin-top:1.125rem}.main .pricing button{margin-top:1.875rem;border:3px solid #724eef;color:#724eef;background-color:transparent;padding:1.625rem 0}@media(min-width:992px){.main .pricing button{margin-top:8.75rem}}.main .pricing__cards{background-color:#f5f5f5;margin-top:1.875rem;padding:1.5rem 1.4375rem}@media(min-width:992px){.main .pricing__cards{margin-top:0}}.main .pricing__cards p{margin-top:1.125rem}.main .pricing__cards a{text-transform:capitalize;text-decoration:underline}.main .pricing__cards ul{list-style-type:none;padding:0;font-weight:300}.main .pricing__cards ul li:before{content:"- "}.main .pricing__cards .pricing__card:first-child{padding-top:0}.main .pricing__cards .pricing__card:last-child{border:none;padding-bottom:0}.main .pricing__card{padding:1.5rem 0;border-bottom:1px solid #c7c7c7}.main .pricing__card .blue{color:#2b2aa7}.main .pricing__card .green{color:#14d071}.main .pricing__card .red{color:#ff635a}@media(min-width:768px){.main .pricing__card{border-bottom:none;border-right:1px solid #c7c7c7;padding:0 1.4375rem}}.main .content{margin:-.9375rem;height:81.25rem}.main .content .dg_card{margin:15px;flex:0 0 17.4375rem}@media(min-width:992px){.main .content .dg_card{max-width:calc(50% - 30px);flex-shrink:0}}.main .content__feature{overflow:hidden;position:relative;display:flex;justify-content:center;align-items:center}.main .content__feature.blue{background-color:#2b2aa7}.main .content__feature.green{background-color:#14d071}.main .content__feature.red{background-color:#ff635a}.main .content__feature img{position:absolute;width:100%;height:100%;object-fit:cover;mix-blend-mode:multiply}.main .content__feature .feature__text{position:absolute;color:#fff;text-align:center;width:70%}.main .content__feature .feature__text h3{font-size:1.625rem;font-weight:200;letter-spacing:.6px}.main .content__customer{display:grid;grid-template-columns:repeat(3,1fr);grid-auto-flow:dense}.main .content__customer .item{display:flex;justify-content:center;align-items:center}.main .content__customer .item::before{content:"";padding-bottom:100%;display:inline-block;vertical-align:top}.main .content__customer .item__airbnb{background-color:#ff5a5f}.main .content__customer .item__adroll{background-color:#f5f5f5}.main .content__customer .item__peleton{background-color:#000}.main .content__customer .item__evernote{background-color:#292929}.main .content__customer .item__samsung{background-color:#1428a0}.main .content__customer .item__nginx{background-color:#00aa4e}.main .content__customer .item__paperduty{background-color:#5dbd5f}.main .content__customer .item__wholefoods{background-color:#000}.main .content__customer .item__att{background-color:#00a8e0}.main .content__customer .item--expand{grid-column:span 2;grid-row:span 2}